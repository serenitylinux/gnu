# vim: ft=sh
name="gnu-build-tools"
iteration="10"
version="1.0.0"
desc="Packages required to build most gnu packages"
bdeps=('gnu-build-tools')
deps=(
	'libc(+dev +glibc)'
	'gcc(+dev +stage3)'
	'binutils(+dev +stage3)'
	'bash'
	'gawk'
	'make'
	'tar'
	'coreutils'
	'sed'
	'grep'
	'gzip'
)
arch=('any')
skip_defaults="true"

function configure() {
	none
}

function build() {
	none
}

function installpkg() {
	none
}
